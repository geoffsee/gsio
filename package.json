{
	"name": "gsio",
	"version": "0.2.10",
	"license": "MIT",
	"bin": "dist/cli.js",
	"workspaces": [
		"packages/llm-memory",
		"packages/llm-memory-mst"
	],
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"dependencies": {
		"@openai/agents-openai": "^0.2.1",
		"llm-memory": "./packages/llm-memory",
		"llm-memory-mst": "./packages/llm-memory-mst",
		"unstorage": "^1.17.1",
		"zod": "3"
	},
	"scripts": {
		"build": "rm -rf ./dist && bun ./build.ts",
		"dev": "bun run build && node dist/cli.js",
		"prepublishOnly": "bun run build",
		"release": "release-it",
		"release-it": "bun test && release-it",
		"release:dry": "release-it --dry-run",
		"release:alpha": "release-it --preRelease=alpha",
		"release:beta": "release-it --preRelease=beta",
		"release:patch": "release-it patch",
		"release:minor": "release-it minor",
		"release:major": "release-it major",
		"container:tty": "docker run -it -v \"$(pwd):/app\" --workdir /app  oven/bun:alpine sh",
		"container:build": "scripts/build-container.sh",
		"container:push": "docker push ghcr.io/geoffsee/gsio:latest-alpine"
	},
	"files": [
		"dist"
	],
	"devDependencies": {
		"@openai/agents": "^0.2.1",
		"@release-it/conventional-changelog": "^10.0.1",
		"@types/react": "^18.0.32",
		"chalk": "^5.2.0",
		"ink": "^4.1.0",
		"ink-testing-library": "^4.0.0",
		"marked": "^16.4.1",
		"meow": "^14.0.0",
		"mobx-state-tree": "^7.0.2",
		"openai": "^6.6.0",
		"prettier": "^3.6.2",
		"react": "^18.2.0",
		"react-devtools-core": "^7.0.1",
		"release-it": "^19.0.6",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"uuid": "^13.0.0",
		"yoga-wasm-web": "^0.3.3"
	}
}
