{
	"name": "gsio",
	"version": "0.2.9-alpha",
	"license": "MIT",
	"bin": "dist/cli.js",
	"workspaces": [
		"packages/llm-memory"
	],
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"dependencies": {
		"@openai/agents-openai": "^0.2.1",
		"llm-memory": "./packages/llm-memory",
		"unstorage": "^1.17.1",
		"zod": "3"
	},
	"scripts": {
		"build": "rm -rf ./dist && bun ./build.ts",
		"dev": "bun run build && node dist/cli.js",
		"prepublishOnly": "bun run build",
		"container:tty": "docker run -it -v \"$(pwd):/app\" --workdir /app  oven/bun:alpine sh",
		"container:build": "scripts/build-container.sh",
		"container:push": "docker push ghcr.io/geoffsee/gsio:latest-alpine"
	},
	"files": [
		"dist"
	],
	"devDependencies": {
		"@openai/agents": "^0.2.1",
		"@types/react": "^18.0.32",
		"chalk": "^5.2.0",
		"ink": "^4.1.0",
		"ink-testing-library": "^3.0.0",
		"marked": "^16.4.1",
		"meow": "^14.0.0",
		"openai": "^6.6.0",
		"prettier": "^2.8.7",
		"react": "^18.2.0",
		"react-devtools-core": "^7.0.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"yoga-wasm-web": "^0.3.3"
	}
}
